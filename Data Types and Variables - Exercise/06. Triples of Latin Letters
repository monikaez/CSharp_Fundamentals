//Create a program that receives an integer n and print all triples
//of the first n small Latin letters, ordered alphabetically:
//Create a program that receives an integer n 
//    and print all triples of the first n small Latin letters,
//    ordered alphabetically:
int n = int.Parse(Console.ReadLine());

for (int a = 0; a < n; a++)
{
	for (int b = 0; b < n; b++)
	{
		for (int c = 0; c < n; c++)
		{
			char first = (char)('a' + a);
			char second = (char)('a' + b);
			char third = (char)('a' + c);
			Console.WriteLine($"{first}{second}{third}");
		} 
	}
}